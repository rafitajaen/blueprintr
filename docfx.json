{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/legacy/**"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net10.0"
      },
      "disableGitFeatures": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ]
      },
      {
        "files": [
          "index.md",
          "toc.yml",
          "docs/**.md",
          "docs/**.yml",
          "README.md",
          "CONTRIBUTING.md",
          "WORKFLOW.md",
          "QUICK_START.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "legacy/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "docs/images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Blueprintr Documentation",
      "_appName": "Blueprintr",
      "_appFooter": "Â© Blueprintr Team - AGPL-3.0 License",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_gitContribute": {
        "repo": "https://github.com/rafitajaen/blueprintr",
        "branch": "main"
      }
    }
  }
}