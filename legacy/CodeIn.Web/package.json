{
    "name": "codein",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "nodemon",
        "build": "tsc && vite build",
        "preview": "vite preview"
    },
    "devDependencies": {
        "@types/node": "^24.5.2",
        "nodemon": "^3.1.10",
        "sass": "^1.93.0",
        "typescript": "~5.9.2",
        "vite": "^7.1.7"
    },
    "nodemonConfig": {
        "watch": [
            "Scripts",
            "Styles",
            "vite.config.ts"
        ],
        "exec": "tsc && vite build",
        "verbose": false,
        "ignore": [
            "node_modules"
        ],
        "ext": "ts, scss",
        "delay": "5s",
        "legacyWatch": "true"
    },
    "dependencies": {
        "axios": "^1.12.2",
        "dotenv": "^17.2.2",
        "path": "^0.12.7"
    }
}
